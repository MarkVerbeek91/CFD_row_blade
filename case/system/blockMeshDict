FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale    1;


xmin    -1.5;
xmax     1.5;
ymin    -1.5;
ymax     1.5;
zmin    -0.05;
zmax     0;

vertices
(
    ( $xmin $ymin $zmin )
    ( $xmax $ymin $zmin )
    ( $xmax $ymax $zmin )
    ( $xmin $ymax $zmin )

    ( $xmin $ymin $zmax )
    ( $xmax $ymin $zmax )
    ( $xmax $ymax $zmax )
    ( $xmin $ymax $zmax )
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ( 60 60 1 ) simpleGrading ( 1 1 1 )
);

edges
(
);

boundary
(
    outlet
    {
        type patch;
        faces
        (
            (0 4 7 3)
        );
    }
    inlet
    {
        type patch;
        faces
        (
            (1 2 6 5)
        );
    }
    ymin
    {
        type symmetry;
        faces
        (
            (0 1 5 4)
        );
    }
    ymax
    {
        type symmetry;
        faces
        (
            (3 7 6 2)
        );
    }
    zmin
    {
        type symmetry;
        faces
        (
            (0 3 2 1)
        );
    }
    atmosphere
    {
        type patch;
        faces
        (
            (4 5 6 7)
        );
    }
);


