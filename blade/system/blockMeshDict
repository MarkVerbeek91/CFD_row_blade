FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale    1;


xmin    -1;
xmax     1;
ymin    -1;
ymax     1;
zmin    -0.05;
zmax     0;
bl_xmin  -.02;
bl_xmax   .02;
bl_ymin  -.1;
bl_ymax   .1;


vertices
(
    ( $xmin $ymin $zmin )
    ( $xmax $ymin $zmin )
    ( $xmax $ymax $zmin )
    ( $xmin $ymax $zmin )

    ( $bl_xmin $bl_ymin $zmin)
    ( $bl_xmax $bl_ymin $zmin)
    ( $bl_xmax $bl_ymax $zmin)
    ( $bl_xmin $bl_ymax $zmin)

    ( $bl_xmin $ymin $zmin)
    ( $bl_xmax $ymin $zmin)
    ( $bl_xmax $ymax $zmin)
    ( $bl_xmin $ymax $zmin)

    ( $xmin $ymin $zmax )
    ( $xmax $ymin $zmax )
    ( $xmax $ymax $zmax )
    ( $xmin $ymax $zmax )

    ( $bl_xmin $bl_ymin $zmax)
    ( $bl_xmax $bl_ymin $zmax)
    ( $bl_xmax $bl_ymax $zmax)
    ( $bl_xmin $bl_ymax $zmax)

    ( $bl_xmin $ymin $zmax)
    ( $bl_xmax $ymin $zmax)
    ( $bl_xmax $ymax $zmax)
    ( $bl_xmin $ymax $zmax)

);

blocks
(
    hex (0 8 11 3 12 20 23 15) ( 20 40 1 ) simpleGrading ( 1 1 1 )
    hex (8 9 5 4 20 21 17 16) ( 1 18 1 ) simpleGrading ( 1 1 1 )
    hex (9 1 2 10 21 13 14 22) ( 20 40 1 ) simpleGrading ( 1 1 1 )
    hex (7 6 10 11 19 18 22 23) ( 1 18 1 ) simpleGrading ( 1 1 1 )
);

edges
(
);

